<h1>Listing avaliacaos</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Usuario</th>
    <th>Empresa</th>
    <th>Situacao avaliacao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @avaliacaos.each do |avaliacao| %>
  <tr>
    <td><%= avaliacao.id %></td>
    <td><%= avaliacao.usuario_id %></td>
    <td><%= avaliacao.empresa_id %></td>
    <td><%= avaliacao.situacao_avaliacao_id %></td>
    <td><%= link_to 'Show', avaliacao %></td>
    <td><%= link_to 'Edit', edit_avaliacao_path(avaliacao) %></td>
    <td><%= link_to 'Destroy', avaliacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Avaliacao', new_avaliacao_path %>
